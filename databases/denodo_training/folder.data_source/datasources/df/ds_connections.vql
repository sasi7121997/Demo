# REQUIRES-PROPERTIES-FILE - # Do not remove this comment!
CREATE OR REPLACE DATASOURCE DF ds_connections
    FOLDER = '/data_source'
    ROUTE LOCAL 'LocalConnection' '${databases.denodo_training.folder.data_source.datasources.df.ds_connections.ROUTE.LOCAL.PATH}' FILENAMEPATTERN = '(.*)\.log'
    TUPLEPATTERN = '\[(.+)Z\]\t([0-9\.]+)\t([a-zA-Z ]+)\t([^,]+),([0-2]+)\(Apache-Coyote/1.1: (.+)\) size:(.+)'
    ENDOFLINEDELIMITER = '\n'
    HEADER = FALSE;

